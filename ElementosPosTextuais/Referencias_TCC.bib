@article{artigo:ASTOLFI1995661,
  title = "Exponential stabilization of nonholonomic systems via discontinuous control",
  journal = "IFAC Proceedings Volumes",
  volume = "28",
  number = "14",
  pages = "661 - 666",
  year = "1995",
  note = "3rd IFAC Symposium on Nonlinear Control Systems Design 1995, Tahoe City, CA, USA, 25-28 June 1995",
  issn = "1474-6670",
  doi = "https://doi.org/10.1016/S1474-6670(17)46904-7",
  url = "\url{http://www.sciencedirect.com/science/article/pii/S1474667017469047}",
  urlaccessdate= {1 maio 2019},
  author = "A. Astolfi",
  keywords = "Nonholonomic Control, Coordinates Transformation, Chained Systems",
  abstract = "In the present work the problem of stabilization of nonholonomic systems is addressed and solved from a completely new point of view. We overcome the obstruction to stabilizability contained in the theorem of Brockett by applying a singular coordinates transformation to the given system. In the transformed space the stabilization problem is trivially solvable. Applying the inverse transformation yields a discontinuous controller, whose properties and performance axe studied in detail. The theory is used to design a discontinuous exponentially stabilizing control law for nonholonomic chained systems."
}

